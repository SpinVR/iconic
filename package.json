{
  "name": "@areo/iconic",
  "version": "1.22.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx @svgr/cli --template component-template.js -d components source --config-file .svgrrc.json --ext tsx",
    "index": "npx cti create components && npx barrelsby --exportDefault --delete --directory components",
    "precompile": "npx rimraf dist  && rm components/index.tsx",
    "compile": "npx tsc",
    "start": "npm run build && npm run index && npm run compile"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@svgr/cli": "^4.3.3",
    "react": "^16.10.2",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "barrelsby": "^2.2.0",
    "create-ts-index": "^1.12.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.4"
  }
}
